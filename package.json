{
  "name": "vite-plugin-figlet",
  "version": "1.0.0",
  "license": "MIT",
  "author": "Leif Arriens",
  "repository": {
    "type": "git",
    "url": "https://github.com/leifarriens/vite-plugin-figlet"
  },
  "homepage": "https://github.com/leifarriens/vite-plugin-figlet",
  "bugs": "https://github.com/leifarriens/vite-plugin-figlet/issues",
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts",
    "release": "pnpm run build && changeset publish",
    "dev": "pnpm build --watch",
    "lint": "tsc",
    "dev:example": "pnpm --filter=*example dev"
  },
  "dependencies": {
    "figlet": "^1.6.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@types/figlet": "^1.5.6",
    "tsup": "^7.1.0",
    "typescript": "^5.1.6",
    "vite": "^4.4.1"
  },
  "keywords": [
    "vite",
    "plugin",
    "vite-plugin",
    "figlet"
  ]
}
