{
  "name": "vite-plugin-figlet",
  "version": "1.3.0",
  "packageManager": "pnpm@9.6.0",
  "license": "MIT",
  "author": "Leif Arriens",
  "repository": {
    "type": "git",
    "url": "https://github.com/leifarriens/vite-plugin-figlet"
  },
  "homepage": "https://github.com/leifarriens/vite-plugin-figlet",
  "bugs": "https://github.com/leifarriens/vite-plugin-figlet/issues",
  "files": [
    "dist"
  ],
  "type": "module",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts",
    "release": "pnpm run build && changeset publish",
    "dev": "pnpm build --watch",
    "lint": "tsc",
    "dev:example": "pnpm --filter=*example dev"
  },
  "peerDependencies": {
    "@types/figlet": "^1.0.0",
    "figlet": "^1.0.0",
    "vite": "^3.0.0 || ^4.0.0 || ^5.0.0 || 6.0.0"
  },
  "dependencies": {
    "figlet": "^1.8.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.10",
    "@types/figlet": "^1.7.0",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vite": "^6.0.0"
  },
  "keywords": [
    "vite",
    "plugin",
    "vite-plugin",
    "figlet"
  ]
}
